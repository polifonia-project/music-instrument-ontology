        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/core/Place"/>
        <rdfs:comment xml:lang="en">The relation between an instrument realization and the place it was built in.</rdfs:comment>
        <rdfs:label xml:lang="en">was built in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/instrument/wasInventedAtTime -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/instrument/wasInventedAtTime">
