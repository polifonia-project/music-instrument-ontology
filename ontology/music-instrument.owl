    <!-- https://w3id.org/polifonia/ontology/instrument/Instrument -->

    <owl:Class rdf:about="https://w3id.org/polifonia/ontology/instrument/Instrument">
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/core/InformationObject"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/music-meta/MediumOfPerformance"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/polifonia/ontology/instrument/wasInventedAtTime"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/core/TimeInterval"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/polifonia/ontology/instrument/wasInventedBy"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/core/Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/arco/ontology/core/hasPart"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/instrument/InstrumentPart"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An instrument intended as an information object (e.g. the guitar as an information object), regardless its physical realizations (e.g. the specific guitar at my place).</rdfs:comment>
        <rdfs:label xml:lang="en">Instrument</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/polifonia/ontology/instrument/InstrumentPart -->

    <owl:Class rdf:about="https://w3id.org/polifonia/ontology/instrument/InstrumentPart">
        <rdfs:comment xml:lang="en">The part of an instrument.</rdfs:comment>
        <rdfs:label xml:lang="en">Instrument part</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/polifonia/ontology/instrument/InstrumentRealization -->

    <owl:Class rdf:about="https://w3id.org/polifonia/ontology/instrument/InstrumentRealization">
        <rdfs:subClassOf rdf:resource="https://w3id.org/polifonia/ontology/core/InformationRealization"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/arco/ontology/location/hasTimeIndexedTypedLocation"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/arco/ontology/location/TimeIndexedTypedLocation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/polifonia/ontology/core/hasName"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/core/Name"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/polifonia/ontology/core/realizes"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/polifonia/ontology/instrument/Instrument"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/polifonia/ontology/instrument/wasBuiltIn"/>
