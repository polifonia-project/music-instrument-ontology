        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/core/TimeInterval"/>
        <rdfs:comment xml:lang="en">The relation between an instrument, intended as an information object, and the time interval it was invented at.</rdfs:comment>
        <rdfs:label xml:lang="en">was invented at time</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/instrument/wasInventedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/instrument/wasInventedBy">
